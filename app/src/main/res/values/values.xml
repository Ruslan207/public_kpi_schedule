<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="card.header_simple_title_main">
        <item name="android:textSize">@dimen/card_header_simple_title_text_size</item>
        <item name="android:textColor">@color/card_text_color_header</item>
        <item name="android:layout_gravity">left|center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/card_header_simple_title_margin_left_main
        </item>
        <item name="android:layout_marginTop">@dimen/card_header_simple_title_margin_top_main</item>
        <item name="android:layout_marginRight">@dimen/card_header_simple_title_margin_right_main
        </item>
        <item name="android:layout_marginBottom">
            @dimen/card_header_simple_title_margin_bottom_main
        </item>
    </style>

    <dimen name="card_header_simple_title_margin_left_main">8dp</dimen>
    <dimen name="card_header_simple_title_margin_top_main">5dp</dimen>
    <dimen name="card_header_simple_title_margin_right_main">8dp</dimen>
    <dimen name="card_header_simple_title_margin_bottom_main">0dp</dimen>
    <dimen name="card_header_outer_view_margin_bottom">0dp</dimen>
    <dimen name="card_base_cardwithlist_list_margin_top">0dp</dimen>

    <dimen name="card_header_native_default_paddingBottom">0dp</dimen>
    <dimen name="card_header_native_default_paddingLeft">0dp</dimen>
    <dimen name="card_header_native_default_paddingRight">0dp</dimen>
    <dimen name="card_header_native_default_paddingTop">0dp</dimen>
    <dimen name="card_main_content_native_cardwithlist_paddingTop">0dp</dimen>


    <style name="expandedTitleTextAppearance">
        <item name="android:textSize">31sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_gravity">right|end</item>
        <item name="android:gravity">end|right</item>
    </style>
</resources>